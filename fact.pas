PROGRAM FACTORIAL;
VAR
	X,MRESULT,RESULT: INTEGER;
PROCEDURE MULT(A,B: INTEGER);
VAR
	I: INTEGER;
	N: INTEGER;
BEGIN
	N:=0;
	FOR I:=1 TO B+1 DO
	BEGIN
		N:=N+A;
	END;
	MRESULT:=N;
END;

PROCEDURE FACT(N: INTEGER);
VAR
	P: INTEGER;
BEGIN
	IF N=1 THEN
	BEGIN
		RESULT:=N;
	END;
	ELSE
	BEGIN
		P:=N-1;
		FACT(P);
		MULT(RESULT,N);
		RESULT:=MRESULT;
	END; 
END;

BEGIN
	READLN(X);
	FACT(X);
	WRITELN(RESULT);
END.
